function calcOfBottles (moneyStart, bottlePrice) {
    var numberOfBottles = Math.floor(moneyStart/bottlePrice);
return numberOfBottles
}

function calcChange(moneyStart, bottlePrice){
    var change=moneyStart%bottlePrice;
    return change;
}


console.log("Hello, Master, Here is your USD " + getMilk(5,1.8)+ " of change and "+ calcOfBottles(5,1.8) + " bottles of milk!");

