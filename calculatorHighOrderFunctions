function add(num1, num2){
    return num1+num2;
}

function multiply (num1, num2){
    return num1*num2;
}

function devide(num1,num2){
    return num1/num2;
}

function subtract(num1, num2){
    return num1-num2;
}

function calculator (num1, num2, operator){ //this is the high order function. It takes as the attribute digits and other functions names
    return operator(num1,num2);
}
