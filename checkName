// This small program allows to check if the person is in the list of the invited to the party



checkName(name);

function checkName(name){
var name=prompt("What is your name? ")
var guestList=["Angela", "Amanda", "Jack", "Joe", "James"];
  if (guestList.includes(name)){
    alert("Welcome, " + name);
  } else {
    alert("Sorry, you are not in the list!");
  }
}
